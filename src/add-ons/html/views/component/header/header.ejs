    <style>

    </style>
</head>
<body>
<script>
    $(function(){
        $('#create_pdf').click(function() {
            html2canvas($('#pdf_wrap')[0]).then(function(canvas) {
                let doc = new jsPDF('p', 'mm', 'a4');
                let imgData = canvas.toDataURL('image/png');
                doc.addImage(imgData, 'PNG', 0, 0);
                doc.setFontSize(10);
                doc.textWithLink('Click Here', 5, 120, {url: 'https://naver.com'});
                doc.save('sample-file.pdf');
            });
        });
    });
</script>
<div class="container">
    <nav class="navbar navbar-expand-lg navbar-light bg-dark">
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mr-auto">

            </ul>
            <div class="pdf_wrap">
            <form class="form-inline my-2 my-lg-0">
                <input class="form-control input-sm mr-sm-2" type="search" placeholder="Page number" aria-label="Search">
                <button id="create_pdf" class="sm-btn btn-outline-success">Search</button>
            </form>
            </div>
        </div>
    </nav>
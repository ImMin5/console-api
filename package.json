{
  "name": "wconsole-server",
  "version": "0.0.1",
  "description": "Clod one Web console API server",
  "scripts": {
    "start": "npm run prod",
    "build": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "server": "babel-node ./src/bin/www.js",
    "server:prod": "node ./dist/bin/www.js",
    "server:dev": "node ./dist/bin/www.js",
    "development": "NODE_ENV=development node ./dist/bin/www.js",
    "stg": "NODE_ENV=staging npm-run-all server",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "NODE_ENV=local nodemon --exec babel-node ./src/app.js",
    "temp": "NODE_ENV=temp nodemon --exec babel-node ./src/app.js",
    "test": "NODE_ENV=temp mocha --require @babel/register ./src/test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.pyengine.net/cloudone/wconsole-server.git"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "express": "^4.16.3",
    "mongoose": "^5.5.7",
    "uuid": "^3.2.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "fs": "^0.0.1-security",
    "mocha": "^6.1.4",
    "chai": "^4.2.0",
    "supertest": "^4.0.2",
    "should": "^13.2.3",
    "assert": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "rand-token": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.4.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-import": "^2.17.2",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "keywords": [],
  "author": "CloudOne"
}

{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "import/prefer-default-export": "off",
        "consistent-return": ["off", { "treatUndefinedAsUnspecified": true }],
        "array-callback-return": "off",
        "no-param-reassign": "off",
        "prefer-destructuring": ["off", {"object": true, "array": false}],
        "max-len": ["error", { "ignoreComments": true }]
    },
    "settings": {
        "import/resolver": {
            "babel-module": {}
        }
    }
}
{
  "cors": [
    "http://*"
  ],
  "grpc": {
    "max_message_length": 268435456
  },
  "timeout": {
    "domainKey": 86400,
    "session": 3600
  },
  "endpoints": {
    "identity": {
      "v1": "identity:50051"
    },
    "repository": {
      "v1": "repository:50051"
    },
    "secret": {
      "v1": "secret:50051"
    },
    "inventory": {
      "v1": "inventory:50051"
    },
    "plugin": {
      "v1": "plugin:50051"
    },
    "monitoring": {
      "v1": "monitoring:50051"
    },
    "statistics": {
      "v1": "statistics:50051"
    },
    "config": {
      "v1": "config:50051"
    }
  },
  "baseURL": "http://localhost:3000",
  "routes": {},
  "authentication": {
    "exclude": [
      "/check",
      "/api-docs",
      "/identity/token/issue",
      "/identity/domain/list",
      "/identity/domain/get",
      "/add-ons/file/download",
      "/add-ons/html/render"
    ]
  },
  "addOns": [{
    "name": "excel",
    "path": "excelInExport",
    "route": "getModules"
  },{
    "name": "pdf",
    "path": "pdfInExport",
    "route": "getModules"
  },{
    "name": "aws-health",
    "path": "awsInExport",
    "route": "getModules"
  },
    {
      "name": "html",
      "path": "htmlInExport",
      "route": "getModules"
    }],
  "redis": {
  },
  "swagger": {
    "api": []
  },
  "logger": {
    "exclude": [
      "/identity/token/issue",
      "/identity/user/create",
      "/identity/user/update",
      "/identity/domain-owner/create",
      "/identity/domain-owner/update",
      "/secret/credential/create"
    ],
    "handlers": [{
      "type": "console",
      "level": "debug"
    }]
  }
}

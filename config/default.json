{
  "cors": [
    "http://localhost:8080"
  ],
  "timeout": {
    "domainKey": 86400,
    "refreshToken": 3600
  },
  "endpoints": {
    "identity": {
      "v1": "identity:50051"
    },
    "repository": {
      "v1": "repository:50051"
    }
  },
  "routes": {
    "identity": "http://localhost:3000",
    "repository": "http://localhost:3000"
  },
  "authentication": {
    "exclude": [
      "/check",
      "/identity/domain/init",
      "/identity/domain/list",
      "/identity/domain/create",
      "/identity/api-key/create",
      "/identity/token/issue"
    ]
  },
  "redis": {
  }
}

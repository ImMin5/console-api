{
  "cors": [
    "http://*"
  ],
  "grpc": {
    "max_message_length": 268435456
  },
  "timeout": {
    "domainKey": 86400,
    "session": 3600
  },
  "requestCache": {
    "enabled": true,
    "ttl": 300
  },
  "requestBodySize": "10mb",
  "endpoints": {
    "identity": {
      "v1": "identity:50051"
    },
    "repository": {
      "v1": "repository:50051"
    },
    "secret": {
      "v1": "secret:50051"
    },
    "inventory": {
      "v1": "inventory:50051"
    },
    "plugin": {
      "v1": "plugin:50051"
    },
    "monitoring": {
      "v1": "monitoring:50051"
    },
    "statistics": {
      "v1": "statistics:50051"
    },
    "config": {
      "v1": "config:50051"
    },
    "report": {
      "v1": "report:50051"
    },
    "power_scheduler": {
      "v1": "power-scheduler:50051"
    },
    "billing": {
      "v1": "billing:50051"
    },
    "spot_automation": {
      "v1": "spot-automation:50051"
    },
    "cost_saving": {
      "v1": "cost-saving:50051"
    },
    "notification": {
      "v1": "notification:50051"
    }
  },
  "baseURL": "http://localhost:3000",
  "routes": {},
  "authentication": {
    "refreshTokenUrl": "/identity/token/refresh",
    "exclude": [
      "/check",
      "/api/reflection",
      "/identity/token/issue",
      "/identity/domain/list",
      "/identity/domain/get",
      "/add-ons/excel/download"
    ]
  },
  "redis": {
  },
  "logger": {
    "exclude": [
      "/check",
      "/api/reflection",
      "/identity/token/issue",
      "/identity/user/create",
      "/identity/user/update",
      "/identity/domain-owner/create",
      "/identity/domain-owner/update",
      "/secret/secret/create"
    ],
    "handlers": [{
      "type": "console",
      "level": "debug"
    }]
  }
}

{
  "cors": [
    "http://*"
  ],
  "timeout": {
    "domainKey": 86400,
    "session": 3600
  },
  "endpoints": {
    "identity": {
      "v1": "identity:50051"
    },
    "repository": {
      "v1": "repository:50051"
    },
    "secret": {
      "v1": "secret:50051"
    },
    "inventory": {
      "v1": "inventory:50051"
    },
    "plugin": {
      "v1": "plugin:50051"
    }
  },
  "baseURL": "http://localhost:3000",
  "routes": {},
  "authentication": {
    "exclude": [
      "/check",
      "/api-docs/",
      "/identity/token/issue",
      "/identity/domain/list",
      "/identity/domain/get",
      "/add-ons/file/download"
    ]
  },
  "addOns": [{
    "name": "excel",
    "path": "excelInExport",
    "route": "getModules"
  },{
    "name": "pdf",
    "path": "pdfInExport",
    "route": "getModules"
  }],
  "redis": {
  },
  "logger": {
    "exclude": [
      "/identity/token/issue",
      "/identity/user/create",
      "/identity/user/update",
      "/identity/domain-owner/create",
      "/identity/domain-owner/update",
      "/secret/credential/create"
    ],
    "handlers": [{
      "type": "console",
      "level": "debug"
    }]
  }
}
